<script>
function openInfoBlock(blockName,elmnt,color) {
    var i, buttoncontent, buttonnotebook2;
    buttoncontent = document.getElementsByClassName("buttoncontent");
    for (i = 0; i < buttoncontent.length; i++) {
        buttoncontent[i].style.display = "none";
    }
    buttonnotebook2 = document.getElementsByClassName("buttonnotebook2");
    for (i = 0; i < buttonnotebook2.length; i++) {
        buttonnotebook2[i].style.backgroundColor = "";
    }
    document.getElementById(blockName).style.display = "block";
    elmnt.style.backgroundColor = color;

}
// Get the element with id="defaultOpen" and click on it
document.getElementById("defaultOpen").click();
</script>

