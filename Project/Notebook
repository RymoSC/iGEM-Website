    var i, buttoncontent17, buttonnotebook17;
    buttoncontent17 = document.getElementsByClassName("buttoncontent17");
    for (i = 0; i < buttoncontent17.length; i++) {
        buttoncontent17[i].style.display = "none";
    }
    buttonnotebook17 = document.getElementsByClassName("buttonnotebook17");
    for (i = 0; i < buttonnotebook17.length; i++) {
        buttonnotebook17[i].style.backgroundColor = "";
    }
    document.getElementById(blockName).style.display = "block";
    elmnt.style.backgroundColor = color;

}
// Get the element with id="defaultOpen17" and click on it
document.getElementById("defaultOpen17").click();
</script>

