<script>
function openInfoBlock(blockName,elmnt,color) {
    var i, buttoncontent1, buttonnotebook4;
    buttoncontent1 = document.getElementsByClassName("buttoncontent1");
    for (i = 0; i < buttoncontent1.length; i++) {
        buttoncontent1[i].style.display = "none";
    }
    buttonnotebook4 = document.getElementsByClassName("buttonnotebook4");
    for (i = 0; i < buttonnotebook4.length; i++) {
        buttonnotebook4[i].style.backgroundColor = "";
    }
    document.getElementById(blockName).style.display = "block";
    elmnt.style.backgroundColor = color;

}
// Get the element with id="defaultOpen2" and click on it
document.getElementById("defaultOpen2").click();
</script>


