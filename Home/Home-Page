<script language="JavaScript">
document.onload = myFunction();
function myFunction() {
   if (window.matchMedia("(min-width:800px)").matches) {
    document.getElementById("myImg").style="width:0%";
    }
    else{
      document.getElementById("video2").style="width:0%"
      document.getElementById("descriptionImage").src="http://2018.igem.org/wiki/images/4/4f/T--UCSC--DescriptionMobile.png"
      document.getElementById("descriptionImage").style="margin-top: -36px;border-top:3px solid black;"
      document.getElementById("firstLink").style="width:50%"
      document.getElementById("secondLink").style="width:50%"
      document.getElementById("thirdLink").style="width:50%;margin-top:2%"
      document.getElementById("fourthLink").style="width:50%;margin-top:5px"
      document.getElementById("firstImage").style="width:150%;"
      document.getElementById("secondImage").style="width:150%;"
      document.getElementById("thirdImage").style="width:150%;float:right"
      document.getElementById("fourthImage").style="width:170%;float:right"
      document.getElementById("firstImage").src="http://2018.igem.org/wiki/images/b/b6/T--UCSC--InfoMobile.png"
      document.getElementById("secondImage").src="http://2018.igem.org/wiki/images/4/48/T--UCSC--TeamImage_Mobile.png"
      document.getElementById("thirdImage").src="http://2018.igem.org/wiki/images/4/4d/T--UCSC--PartnershipMobile.png"
      document.getElementById("fourthImage").src="http://2018.igem.org/wiki/images/a/aa/T--UCSC--ITESG_Mobile.png"
      document.getElementById("video-class").style="top:60%;left:66%"
      document.getElementById("logoImage").style="width:150%"
      document.getElementById("middle-text-class1").style="font-size:0px;"
      document.getElementById("middle-text-class2").style="font-size:0px;"
      document.getElementById("middle-text-class3").style="font-size:0px;"
      document.getElementById("middle-text-class4").style="font-size:0px;"
    }
}
</script>

